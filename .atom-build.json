{
  "cmd": "gnatmake -F -D \"{PROJECT_PATH}/build\" -o \"{PROJECT_PATH}/build/{FILE_ACTIVE_NAME_BASE}\" -gnat95 {FILE_ACTIVE}",
  "cwd": "{FILE_ACTIVE_PATH}",
  "env": {
    "PATH": "/usr/local/gnat/bin:/usr/bin"
